<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
	  <Version>1.0.4</Version>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
	  <Authors>
		  Chandan Bhagat
	  </Authors>
	  <RepositoryUrl>https://github.com/thechandanbhagat/dotnet-essesntials.git</RepositoryUrl>
	  <Title>Dotnet Essentials</Title>
	  <PackageProjectUrl>https://github.com/thechandanbhagat/dotnet-essesntials</PackageProjectUrl>
	  <RepositoryType>git</RepositoryType>
	  <PackageTags>dotnet;essentials;</PackageTags>
	  <PackageId>dotnet-essential</PackageId>
	  <PackageIcon>_fd10e521-0fd2-4500-872e-e746c1b91a25.jpg</PackageIcon>
